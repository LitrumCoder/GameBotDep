run = "dotnet run"
entrypoint = "Program.cs"

[env]
DOTNET_NOLOGO = "1"
DOTNET_CLI_TELEMETRY_OPTOUT = "1"

[nix]
channel = "stable-23_11"

[languages.csharp]
pattern = "**/*.cs"
syntax = "csharp"

[languages.csharp.languageServer]
start = ["omnisharp", "--languageserver"]

[packager]
language = "dotnet"

[packager.features]
packageSearch = true

[deployment]
run = ["sh", "-c", "dotnet run"]
deploymentTarget = "cloudrun" 